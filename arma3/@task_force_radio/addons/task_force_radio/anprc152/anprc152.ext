{
	idc = IDC_ANPRC152_BACKGROUND;
	text = "\task_force_radio\anprc152\152.paa";
	x = 0.0138687 * safezoneW + safezoneX;
	y = -1.28145 * safezoneH + safezoneY;
	w = 0.335981 * safezoneW;
	h = 2.2902 * safezoneH;
	moving = 1;
};
