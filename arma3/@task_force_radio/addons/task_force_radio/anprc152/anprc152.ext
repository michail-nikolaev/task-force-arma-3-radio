	idc = IDC_ANPRC152_EDIT;
	x = 0.181588 * safezoneW + safezoneX;
	y = 0.368371 * safezoneH + safezoneY;
	w = 0.0653438 * safezoneW;
	h = 0.043352 * safezoneH;
	tooltip = $STR_current_freq;
	canModify = 1;
};
