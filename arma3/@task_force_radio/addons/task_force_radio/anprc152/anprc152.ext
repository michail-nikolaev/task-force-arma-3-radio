	y = 0.4824 * safezoneH + safezoneY;
	w = 0.0599875 * safezoneW;
	h = 0.043175 * safezoneH;
	font = "tf_font_dots";
	shadow = 2;
	tooltip = $STR_current_freq;
	canModify = 1;
};
class channel_edit: RscEditLCD
{
	moving = 1;
