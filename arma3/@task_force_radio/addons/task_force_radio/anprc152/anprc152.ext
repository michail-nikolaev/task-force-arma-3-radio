class background: RscBackPicture
{
	idc = IDC_ANPRC152_BACKGROUND;
	text = "\task_force_radio\anprc152\152.paa";
	x = -0.089575 * safezoneW + safezoneX;
	y = -0.0293181 * safezoneH + safezoneY;
	w = 0.56175 * safezoneW;
	h = 0.988621 * safezoneH;
	moving = 1;
};
