class pnr1000a_radio_dialog
{
	idd = IDC_PNR1000A_RADIO_DIALOG;
	movingEnable = 1;
	controlsBackground[] = { };
	objects[] = { };
	onUnload = "['OnRadioOpen', player, [player, TF_sw_dialog_radio, false, 'pnr1000a_radio_dialog', false]] call TFAR_fnc_fireEventHandlers;";
	onLoad = "if(sunOrMoon < 0.2)then{((_this select 0) displayCtrl TF_BACKGROUND_ID) ctrlSetText '\task_force_radio\pnr1000a\pnr1000a_n.paa';((_this select 0) displayCtrl TF_MICRODAGR_BACKGROUND_ID) ctrlSetText '\task_force_radio\microdagr\microdagr_n.paa';}; _this call TFAR_fnc_updateProgrammatorDialog;";
	controls[]=
	{
		background,
		prev_channel,
		next_channel,
		increase_volume,
		decrease_volume,
		stereo,
		reset_channel,
		speakers,
		microdagr_background,
		channel_edit,
		edit,
		clear,
		enter
	};
	////////////////////////////////////////////////////////
	// GUI EDITOR OUTPUT START (by Kavinsky, v1.063, #Jafaxu)
	////////////////////////////////////////////////////////

	class background: RscBackPicture
	{
		idc = IDC_PNR1000A_BACKGROUND;
		text = "\task_force_radio\pnr1000a\pnr1000a.paa";
		x = -0.0153696 * safezoneW + safezoneX;
		y = -0.323109 * safezoneH + safezoneY;
		w = 0.38292 * safezoneW;
		h = 1.31752 * safezoneH;
		moving = 1;
	};
	class prev_channel: HiddenButton
	{
		idc = IDC_PNR1000A_PREV_CHANNEL;
