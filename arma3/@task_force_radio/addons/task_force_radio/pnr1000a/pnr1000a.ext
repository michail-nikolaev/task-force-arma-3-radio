		idc = IDC_MICRODAGR_CHANNEL_EDIT;
		text = "CH1";
		x = 0.437609 * safezoneW + safezoneX;
		y = 0.3504 * safezoneH + safezoneY;
		w = 0.0381562 * safezoneW;
		h = 0.0528 * safezoneH;
		moving = 1;
		font = "tf_font_dots";
		shadow = 2;
		canModify = 0;
		tooltip = $STR_current_channel;
	};
	class edit: RscEditLCD
	{
		idc = IDC_MICRODAGR_EDIT;
