		text = "CLR";
		x = 0.435547 * safezoneW + safezoneX;
		y = 0.4329 * safezoneH + safezoneY;
		w = 0.0402187 * safezoneW;
		h = 0.0517 * safezoneH;
		tooltip = $STR_clear_frequency;
		font = "tf_font_dots";
		shadow = 2;
		action = "ctrlSetText [TF_MICRODAGR_EDIT_ID, '']; ctrlSetFocus ((findDisplay IDC_PRN1000A_RADIO_DIALOG_ID) displayCtrl TF_MICRODAGR_EDIT_ID);";
	};
	class enter: HiddenButton
	{
		idc = IDC_MICRODAGR_ENTER;
		text = "ENT";
